{
    "version": 2,
    "builds": [
        {
            "src": "VercelDeploy/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "VercelDeploy/wsgi.py"
        }
    ],
    "buildCommand": "python manage.py migrate --no-input && python manage.py create_superuser.py",
    "installCommand": "pip install -r requirements.txt"
}