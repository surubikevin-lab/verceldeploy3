{
    "version": 2,
    "builds": [
        {
            "src": "VercelDeploy/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "VercelDeploy/wsgi.py"
        }
    ],
    "buildCommand": "bash vercel-build.sh",
    "outputDirectory": "."
}